{
  "$schema": "https://json.schemastore.org/prettierrc",
  "semi": false,
  "tabWidth": 2,
  "singleQuote": true,
  "printWidth": 90,
  "bracketSpacing": true,
  "trailingComma": "none",
  "arrowParens": "avoid",
  "htmlWhitespaceSensitivity": "css",
  "jsxBracketSameLine": false,
  "proseWrap": "always",
  "no-multiple-empty-lines": [
    "error",
    {
      "max": 1,
      "maxEOF": 0
    }
  ]
}
